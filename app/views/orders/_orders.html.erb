<table class="table table-striped">
  <thead>
    <tr>
      <th><%= Order.human_attribute_name(:id) %></th>
      <th><%= Order.human_attribute_name(:what) %></th>
      <th><%= Order.human_attribute_name(:price) %></th>
      <th><%= Order.human_attribute_name(:category_id) %></th>
      <th><%= Order.human_attribute_name(:user_id) %></th>
      <th><%= Order.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
      <%= render @orders %>
  </tbody>
</table>
